{
  "name": "day-tracker-app",
  "version": "1.0.0",
  "description": "Day tracker app",
  "scripts": {
    "install": "(cd packages/back && npm install) && (cd packages/front && npm install)",
    "backend": "cd packages/back && npm install",
    "frontend": "cd packages/front && npm install",
    "start": "(cd packages/back && npm run start) && (cd packages/front && npm run start)",
    "dev": "(cd packages/back && npm run dev) && (cd packages/front && npm start)",
    "build:front": "npm --prefix packages/front run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axelcedercreutz/day-tracker-app.git"
  },
